{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "max-line-length": {
            "options": [238]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        },
        //Dev
        "no-empty": false,
        "no-unused-expression": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [238]
        }
    }
}
